{
	"extends": "expo/tsconfig.base",
	"compilerOptions": {
		"strict": true,
		"paths": {
			"@/*": ["./*"],
			"@/src/*": ["./src/*"],
			"@/components/*": ["./src/components/*"],
			"@/constants/*": ["./src/constanst/*"],
			"@/context/*": ["./src/context/*"],
			"@/hooks/*": ["./src/hooks/*"],
			"@/interface/*": ["./src/interface/*"],
			"@/service/*": ["./src/service/*"]
		}
	},
	"include": ["**/*.ts", "**/*.tsx"]
}
